{
    "name": "acme-inc/craft-acme-shipping-method",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "support@acme.inc"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.4.5"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "acmeinc\\craftacmeshippingmethod\\": "src/"
        }
    },
    "extra": {
        "handle": "acme-shipping-method",
        "name": "Acme Shipping Method",
        "developer": "Acme Inc",
        "documentationUrl": ""
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

